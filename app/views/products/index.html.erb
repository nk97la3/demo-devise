<p id="notice"><%= notice %></p>

<h1 align="center">Posts</h1>
<div class="aaa" align="center">
<table>
  <tbody>
    <% @products.each do |product| %>
      <%-if product.published == "published" %>
        <tr>
          <td><font size="5"><%= link_to product.title,product %></font></td>
          <td><%= product.opening %> <%= product.created_at%> </td>
        </tr>
      <%end%>      
    <% end %>
  </tbody>
</table>
</div>
<br>

